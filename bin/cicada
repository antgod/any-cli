#!/usr/bin/env node
const init = require('../lib/command/init')
const config = require('../config/templates.json')
const program = require('commander')

program
  .command('init')
  .description('创建用户自定义组件脚手架')
  .alias('i')
  .action(init.default.bind(null, config))

program.parse(process.argv)
if(program.args.length === 0){
  program.help()
}